{
  "fixme + fm": {
    "body": "-- FIXME: ",
    "prefix": [
      "fixme",
      "fix"
    ]
  },
  "hack": {
    "body": "-- HACK: ",
    "prefix": "hack"
  },
  "ignore + stylua + styluaignore": {
    "body": "-- stylua: ignore",
    "prefix": [
      "ignore",
      "stylua",
      "styluaignore"
    ]
  },
  "lazykeys + keys": {
    "body": [
      "  -- stylua: ignore",
      "  keys = {",
      "",
      "  },"
    ],
    "prefix": [
      "lazykeys",
      "keys"
    ]
  },
  "newlspconfig + lspconf": {
    "body": [
      "---@type vim.lsp.Config",
      "return {",
      "  ---@module \"codesettings\"",
      "  ---@type lsp.${1:server-name}",
      "  settings = {",
      "    ${0:}",
      "  },",
      "}"
    ],
    "prefix": [
      "newlspconfig",
      "lspconf"
    ]
  },
  "newpluginspec + pluginspec": {
    "body": [
      "---@type LazyPluginSpec",
      "return {",
      "  \"$1\",",
      "  version = \"${2:*}\",",
      "  -- stylua: ignore",
      "  keys = {",
      "  ",
      "  },",
      "  opts = {},",
      "}"
    ],
    "prefix": "newpluginspec"
  },
  "note": {
    "body": "-- NOTE: ",
    "prefix": "note"
  },
  "perf + optim": {
    "body": "-- OPTIMIZE: ",
    "prefix": [
      "perf",
      "optim"
    ]
  },
  "pluginspectype + pspectype": {
    "body": "---@type LazyPluginSpec",
    "prefix": [
      "pluginspec",
      "lazypluginspec"
    ]
  },
  "snacksspec": {
    "body": [
      "---@type LazyPluginSpec",
      "return {",
      "  \"snacks.nvim\",",
      "  -- stylua: ignore",
      "  keys = {",
      "  ",
      "  },",
      "  ---@type snacks.Config",
      "  opts = {",
      "    ",
      "  },",
      "}"
    ],
    "prefix": "snacksspec"
  },
  "test": {
    "body": "-- TEST: ",
    "prefix": "test"
  },
  "todo": {
    "body": "-- TODO: ",
    "prefix": [
      "todo",
      "to"
    ]
  },
  "warn + warning": {
    "body": "-- WARNING: ",
    "prefix": [
      "warn",
      "warning"
    ]
  }
}
